{
    "name": "ixocreate/security-header",
    "description": "IXOCREATE Security Header Library",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE",
            "email": "opensource@ixocreate.com",
            "homepage": "https://www.ixocreate.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^2.0",
        "ixocreate/test": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\SecurityHeader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ixocreate\\Misc\\SecurityHeader\\": "tests/misc/",
            "Ixocreate\\Test\\SecurityHeader\\": "tests/src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
